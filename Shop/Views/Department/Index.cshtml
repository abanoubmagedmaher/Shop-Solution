@model List<Department>
@{
    ViewData["Title"] = "Index";
}
<h1>Index</h1>
<a href="/Department/AddDept" class="btn btn-success">Add+</a>
<table class="table table-hover table-bordered">
    <tr>
        <th>Name</th>
        <th>Manager Name</th>
    </tr>
    @foreach(var dept in Model){
      <tr>
       <td>@dept.Name</td>
       <td>@dept.ManagerName</td>
    </tr>
    }

</table>
