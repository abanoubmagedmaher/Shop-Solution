@model List<Employee>
@{
    ViewData["Title"] = "Index";
}
<a href="/Employee/AddEmployee" class="btn btn-success m-2">Add+</a>
	<!-- Row -->
						<div class="row">
							<div class="col-md-12 col-lg-12">
								<div class="card">
									<div class="card-header">
										<h3 class="card-title">Employee Table</h3>
									</div>
									<div class="table-responsive">
										<table class="table card-table table-vcenter text-nowrap">
											<thead >
												<tr>
													<th>ID</th>
													<th>Name</th>
													<th>Mobile</th>
													<th>JobTitle</th>
													<th>ImageURL</th>
													<th>Edit</th>
													<th>Delete</th>
												</tr>
											</thead>
											<tbody>
												@foreach(var Emp in Model)
												{
												<tr>
													<th scope="row">@Emp.Id</th>
													<th >@Emp.Name</th>
													<td>@Emp.Mobile</td>
													<td>@Emp.JobTitle</td>
													<td>@Emp.ImageURL</td>
													<td>

														<a href="/Employee/Edit/@Emp.Id">Edit </a>
													<td>
														<td>
														<a href="/Employee/Edit/@Emp.Id">Delete</a>
													<td>
												</tr>
												}
											</tbody>
										</table>
									</div>
									<!-- table-responsive -->
								</div>
							</div>
						</div>
						<!-- End Row -->

